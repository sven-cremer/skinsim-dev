<?xml version="1.0" ?>
<sdf version='1.4'>
    <model name='plunger'>
        <pose>0 0 0.105 0 0 0</pose>
        <static>false</static>
        
        <link name='plunger_link'>
        
          <inertial>
              <mass>1</mass>
              <inertia>
                  <ixx>0.145833</ixx>
                  <ixy>0</ixy>
                  <ixz>0</ixz>
                  <iyy>0.145833</iyy>
                  <iyz>0</iyz>
                  <izz>0.125</izz>
              </inertia>
          </inertial>
          
          <collision name='plunger_collision'>
              <geometry>
                  <cylinder>
                        <radius>0.03</radius>
                        <length>0.10</length>
                  </cylinder>
              </geometry>
              <max_contacts>200</max_contacts>
              <surface>
                <contact>
                  <ode/>
                </contact>
                <bounce/>
                <friction>
                  <ode/>
                </friction>
              </surface>
          </collision>
          
          <visual name='plunger_visual'>
              <geometry>
                  <cylinder>
                      <radius>0.03</radius>
                      <length>0.10</length>
                  </cylinder>
              </geometry>
              <material>
                <script>
                  <uri>file://media/materials/scripts/gazebo.material</uri>
                  <name>Gazebo/Blue</name>
                  <!--name>Gazebo/BlueTransparent</name-->
                </script>
              </material>
          </visual>
          
          <sensor name='plunger_sensor' type='contact'>
              <contact>
                  <collision>plunger_collision</collision>
              </contact>
              <!--update_rate> 200 </update_rate-->
          </sensor>
          
          <self_collide>0</self_collide>
          <kinematic>0</kinematic>
          <gravity>0</gravity>
          
        </link>
                  
        <joint name='plunger_joint' type='prismatic'>
          <parent>world</parent>
          <child>plunger_link</child>
          <axis>
              <xyz>0 0 1</xyz>
              <limit>
                  <lower>-0.5</lower>
                  <upper>2</upper>
              </limit>
              <use_parent_model_frame>0</use_parent_model_frame>
          </axis>
          <physics>
             <provide_feedback>1</provide_feedback>
             <ode>
                <implicit_spring_damper>0</implicit_spring_damper>
             </ode>
          </physics>
        </joint>
          
        <plugin name='Plunger' filename='libPlunger.so'>
            <file_name>plunger</file_name>
            <rosNamespace>skinsim</rosNamespace>
            <updateRate>0</updateRate>
            <Kp>0.1</Kp>
            <Kd>0.0</Kd>
            <Kv>0.1</Kv>
            <JointPgain>0.5</JointPgain>
            <JointIgain>0.01</JointIgain>
            <JointDgain>0.0</JointDgain>
        </plugin>
    
    </model>
</sdf>

